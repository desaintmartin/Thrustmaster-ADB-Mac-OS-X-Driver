display dialog "Are you sure you want to disable the AppleADBButton's extension?" buttons {"Cancel", "Disable"} default button "Cancel"if button returned of result is equal to "Disable" then		set command to "/bin/mkdir /System/Library/Extensions-Disabled"	set command to command & "; " & "/bin/mv /System/Library/Extensions/AppleADBButtons.kext /System/Library/Extensions-Disabled/AppleADBButtons.kext"	do shell script "sh -c " & quoted form of command with administrator privileges	end if